{
  "name": "vue3-admin",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:open": "vue-cli-service serve --open",
    "serve:debug": "vue-cli-service serve --mode debug",
    "serve:mock": "vue-cli-service serve --mode mock",
    "dev": "vue-cli-service serve",
    "dev:open": "vue-cli-service serve --open",
    "dev:debug": "vue-cli-service serve --mode debug",
    "dev:mock": "vue-cli-service serve --mode mock",
    "report": "vue-cli-service build --report && serve ./dist/report.html",
    "build": "vue-cli-service build --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode test",
    "build:preview": "vue-cli-service build --mode production && serve ./dist",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "stylelint": "stylelint",
    "stylelint:fix": "stylelint src/**/*.{css,less,scss} --fix",
    "commit": "git add . && cz",
    "push": "git add . && cz && git push",
    "release": "release-it",
    "release:detail": "release-it --dry-run",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "check-node-version": "node check-node-version.mjs",
    "postinstall": "npm run check-node-version"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "core-js": "^3.30.2",
    "element-plus": "^2.3.5",
    "js-cookie": "^3.0.5",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.3",
    "screenfull": "^6.0.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@iconify-json/ep": "^1.1.10",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.2.5",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^27.0.0-alpha.3",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^27.0.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.14.1",
    "glob": "^10.2.6",
    "jest": "^27.5.1",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "release-it": "^15.10.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "semver": "^7.5.4",
    "serve": "^14.2.0",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-less": "^1.0.8",
    "stylelint-order": "^6.0.3",
    "svg-sprite-loader": "^6.0.11",
    "ts-jest": "^29.1.1",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.25.0",
    "vue-eslint-parser": "^9.3.0",
    "webpack": "^5.85.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">= 7.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs/vue-next",
    "element-ui",
    "element-plus",
    "vue3-admin",
    "webpack5",
    "mock",
    "commitizen",
    "lint-staged",
    "commitlint",
    "pinia",
    "typescript",
    "release-it",
    "changelog",
    "axios"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mr-Super-X/vue3-admin.git"
  }
}
